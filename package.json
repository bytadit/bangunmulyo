    "scripts": {
        "dev": "vite",
        "start": "electron .",
        "build": "electron-builder"
    },
    "build": {
        "appId": "com.example.my-laravel-app",
        "productName": "Bangun Mulyo Fix",
        "directories": {
            "output": "dist"
        },
        "mac": {
            "category": "your.app.category.type"
        },
        "win": {
